citHeader("To cite jointVIP in publications use:")

citEntry(
  entry    = "Article",
  title    = "{jointVIP}: Prioritizing variables in observational study design with joint variable importance plot in R",
  author   = c(person(c("Lauren", "D."), "Liao"),
                    person(c("Samuel", "D."), "Pimentel")),
  journal  = "arXiv",
  year     = 2023,
  note = {"R package version 0.1.2"},
  url      = "https://arxiv.org/abs/2302.10367",
  textVersion = paste0("Lauren D Liao and Samuel D. Pimentel (2023). ",
                "jointVIP: Prioritizing variables in observational study design with joint variable importance plot in R. ",
                "arXiv. URL https://arxiv.org/abs/2302.10367")
)
